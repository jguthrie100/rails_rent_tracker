<h3>List of Tenants</h3>
<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Payment Handle</th>
      <th>Phone Number</th>
      <th>Email</th>
      <th>House</th>
    </tr>
  </thead>
  <tbody>
    <% @tenants.each do |t| %>
      <tr>
        <td><%= link_to(t.name, tenant_path(t)) %></td>
        <td><%= t.payment_handle %></td>
        <td><%= t.phone_num %></td>
        <td><%= t.email %></td>
        <td><%= t.house.nil? ? "" : link_to(t.house.name, house_path(t.house)) %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<p>
  <%= link_to "Add/Edit Tenants", edit_tenants_path %>
</p>